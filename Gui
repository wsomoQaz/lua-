        enabled = not enabled
        ToggleBtn.Text = enabled and "åœæ­¢è‡ªåŠ¨æ”»å‡»" or "å¯åŠ¨è‡ªåŠ¨æ”»å‡»"
        ToggleBtn.BackgroundColor3 = enabled and Color3.fromRGB(200, 50, 50) or Color3.fromRGB(60, 180, 75)
end)

-- æ‰§è¡Œå‚æ•°
local args = {
        13,
        "Standard",
        false,
        "Silver Pickaxe",
        0.85
}

-- ä¸»å¾ªç¯ï¼šæ¯ç§’50æ¬¡
spawn(function()
        while true do
                if enabled then
                        for i = 1, 50 do
                                -- å®‰å…¨æ‰§è¡Œä¸€æ¬¡è°ƒç”¨
                                pcall(function()
                                        ReplicatedStorage:WaitForChild("RemoteEvent"):WaitForChild("HitBox"):FireServer(unpack(args))
                                end)
                        end
                end
                wait(1)
        end
end)
    end
}) 

