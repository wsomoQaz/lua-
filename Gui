        folded = not folded
        Frame.Size = folded and UDim2.new(0, 200, 0, 40) or UDim2.new(0, 200, 0, 100)
        FoldBtn.Text = folded and "å±•å¼€é¢æ¿" or "æŠ˜å é¢æ¿"
