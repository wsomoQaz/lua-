        and Workspace.OreArea["Quartz-Cave"]["StoneArea-Layer1"].Amount:FindFirstChild("QuartzStone")

    if quartzFolder then
        for _, obj in pairs(quartzFolder:GetChildren()) do
            if obj:IsA("BasePart") and obj.Name == "Center" then
                table.insert(centers, obj)
            end
        end
    end

    return centers
end

